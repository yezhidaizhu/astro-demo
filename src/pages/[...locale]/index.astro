---
import { Hello } from "../../components/Hello";
import SEOPage from "../../layouts/SEOPage.astro";
import { defineStaticPaths } from "../../utils/seo";

export function getStaticPaths() {
  return defineStaticPaths();
}

const { curPageLocales } = Astro.props;
const seoI18nNamespace = "demo";
---

